{
    "family": "flashcards-task",
    "containerDefinitions": [
      {
        "name": "flashcards-container",
        "image": "<ECR_IMAGE_URI>", 
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 5000,
            "hostPort": 5000
          }
        ]
      }
    ],
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
  }
  